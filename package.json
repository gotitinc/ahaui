{
  "name": "ahaui3",
  "version": "3.0.1",
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "build:css": "turbo run build  --filter=./packages/css",
    "build:icons": "turbo run build  --filter=./packages/icons",
    "build:react": "turbo run build  --filter=./packages/react",
    "build:docs": "turbo run build --filter=./apps/docs",
    "dev": "turbo run dev --parallel",
    "dev:docs": "turbo run dev --filter=./apps/docs",
    "dev:css": "turbo run dev  --filter=./packages/css",
    "dev:icons": "turbo run dev  --filter=./packages/icons",
    "dev:react": "turbo run dev  --filter=./packages/react",
    "lint": "turbo run lint",
    "deploy": "turbo run deploy",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "version": "turbo changeset version"
  },
  "devDependencies": {
    "babel-eslint": "10.1.0",
    "babel-jest": "^29.5.0",
    "babel-loader": "^9.1.2",
    "babel-plugin-module-resolver": "^5.0.0",
    "babel-preset-react-app": "^10.0.1",
    "babel-runtime": "^6.26.0",
    "@babel/core": "^7.21.0",
    "@babel/plugin-external-helpers": "^7.18.6",
    "@babel/plugin-syntax-class-properties": "7.12.13",
    "@babel/plugin-syntax-dynamic-import": "7.8.3",
    "@babel/plugin-syntax-optional-chaining": "7.8.3",
    "@babel/plugin-transform-runtime": "^7.21.0",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-react": "^7.18.6",
    "@babel/runtime": "^7.21.0",
    "autoprefixer": "10.4.13",
    "eslint": "8.35.0",
    "eslint-config-airbnb": "19.0.4",
    "eslint-config-prettier": "8.7.0",
    "eslint-config-react-app": "7.0.1",
    "eslint-loader": "4.0.2",
    "eslint-plugin-flowtype": "^8.0.3",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jsx-a11y": "6.7.1",
    "eslint-plugin-react": "7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "fs-extra": "11.1.0",
    "husky": ">=8.0.3",
    "rollup": "^2.75.3",
    "rollup-plugin-eslint": "^7.0.0",
    "rollup-plugin-includepaths": "^0.2.4",
    "rollup-plugin-node-externals": "^5.1.2",
    "rollup-plugin-terser": "^7.0.2",
    "@rollup/plugin-alias": "^4.0.3",
    "@rollup/plugin-babel": "^6.0.3",
    "@rollup/plugin-commonjs": "^24.0.1",
    "@rollup/plugin-node-resolve": "^15.0.1",
    "@rollup/plugin-typescript": "^11.0.0",
    "turbo": "latest",
    "vite": "^2.9.1",
    "webpack": "^4.43.0"
  },
  "engines": {
    "npm": ">=9.0.0",
    "node": ">=16.0.0"
  },
  "author": "Got It, Inc.",
  "homepage": "https://github.com/gotitinc/ahaui#readme",
  "slack": "https://ahaui.slack.com/join/shared_invite/zt-18rg4yd00-B6ob9D2DgXXVILSWh6CNrQ#/shared-invite/email",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gotitinc/ahaui.git"
  },
  "packageManager": "yarn@1.22.18"
}
