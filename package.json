{
  "name": "ahaui",
  "version": "2.1.6",
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "build": "turbo run build --filter=@ahaui/react --filter=@ahaui/css --parallel",
    "build:css": "turbo run build  --filter=@ahaui/css",
    "build:icons": "turbo run build  --filter=@ahaui/icons",
    "build:react": "turbo run build  --filter=@ahaui/react",
    "dev": "pnpm run build:css && start-storybook -p 6006 --quiet",
    "dev:css": "turbo run dev  --filter=@ahaui/css",
    "dev:icons": "turbo run dev  --filter=@ahaui/icons",
    "dev:react": "turbo run dev  --filter=@ahaui/react",
    "lint": "turbo run lint",
    "deploy": "turbo run deploy",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "version": "turbo changeset version"
  },
  "dependencies": {
    "react": "17.0.2",
    "react-dom": "17.0.2"
  },
  "devDependencies": {
    "@types/node": "^18.0.2",
    "@types/react": "^17.0.2",
    "@typescript-eslint/eslint-plugin": "5.38.0",
    "@typescript-eslint/parser": "5.38.0",
    "@vitejs/plugin-react": "^2.0.1",
    "eslint": "^8.0.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-config-react-app": "^7.0.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "husky": ">=4",
    "lint-staged": "10.5.2",
    "prettier": "^2.7.1",
    "turbo": "latest",
    "typescript": "^4.9.5",
    "vite": "^3.0.7",
    "vite-tsconfig-paths": "^3.5.0",
    "@vitejs/plugin-react-swc": "^3.0.0",
    "@storybook/addon-a11y": "^6.4.9",
    "@storybook/react": "^6.4.9",
    "@storybook/builder-vite": "0.4.2",
    "@storybook/addons": "6.4.9",
    "@storybook/addon-links": "6.4.9",
    "@storybook/theming": "6.4.9",
    "@storybook/preset-scss": "^1.0.3",
    "@storybook/testing-library": "^0.0.13",
    "@testing-library/jest-dom": "^5.14.1",
    "@storybook/preview-web": "6.4.9",
    "@storybook/client-api": "6.4.9",
    "@storybook/channel-postmessage": "6.4.9",
    "@storybook/addon-essentials": "6.4.9",
    "@storybook/addon-docs": "6.4.9",
    "@storybook/addon-actions": "6.4.9",
    "@storybook/addon-backgrounds": "6.4.9",
    "@storybook/addon-measure": "6.4.9",
    "@storybook/addon-outline":"6.4.19",
    "@storybook/channel-websocket": "6.4.9",
    "react-docgen": "5.4.3",
    "sass": "^1.43.4",
    "sass-loader": "^12.3.0",
    "css-loader":"6.7.3"
  },
  "author": "Got It, Inc.",
  "homepage": "https://github.com/gotitinc/ahaui#readme",
  "slack": "https://ahaui.slack.com/join/shared_invite/zt-18rg4yd00-B6ob9D2DgXXVILSWh6CNrQ#/shared-invite/email",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gotitinc/ahaui.git"
  },
  "packageManager": "^pnpm@7.28.0"
}
