{
  "name": "ahaui3",
  "version": "3.0.1",
  "private": true,
  "scripts": {
    "build": "turbo run build",
    "build:css": "turbo run build  --filter=./packages/css",
    "build:icons": "turbo run build  --filter=./packages/icons",
    "build:react": "turbo run build  --filter=./packages/react",
    "build:docs": "turbo run build --filter=./apps/docs",
    "dev": "turbo run dev --parallel",
    "dev:docs": "turbo run dev --filter=./apps/docs",
    "dev:css": "turbo run dev  --filter=./packages/css",
    "dev:icons": "turbo run dev  --filter=./packages/icons",
    "dev:react": "turbo run dev  --filter=./packages/react",
    "lint": "turbo run lint",
    "deploy": "turbo run deploy",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "version": "turbo changeset version"
  },
  "devDependencies": {
    "fs-extra": "7.0.1",
    "husky": "7.0.0",
    "lint-staged": "10.5.2",
    "prettier": "latest",
    "turbo": "latest"
  },
  "engines": {
    "npm": ">=7.0.0",
    "node": ">=14.0.0"
  },
  "author": "Got It, Inc.",
  "homepage": "https://github.com/gotitinc/ahaui#readme",
  "slack": "https://ahaui.slack.com/join/shared_invite/zt-18rg4yd00-B6ob9D2DgXXVILSWh6CNrQ#/shared-invite/email",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gotitinc/ahaui.git"
  }
}
