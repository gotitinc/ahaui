{
  "name": "@ahaui/icons",
  "version": "2.1.0-alpha.0",
  "main": "lib/index.js",
  "module": "es/index.js",
  "files": [
    "es/*.{js,map}",
    "lib/*.{js,map}",
    "CHANGELOG.md"
  ],
  "scripts": {
    "build": "rollup -c",
    "dev": "rollup -c -w",
    "prepublishOnly": "yarn build",
    "clean": "rimraf lib es",
    "format": "eslint --ext js src --fix",
    "lint": "eslint --ext js src",
    "prepack": "clean-package -rm scripts",
    "postpack": "clean-package restore"
  },
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@ahaui/react": "*"
  },
  "peerDependencies": {
    "@ahaui/react": "*"
  },
  "lint-staged": {
    "*.js": "eslint --cache --fix"
  },
  "prettier": {
    "singleQuote": true,
    "trailingComma": "all"
  },
  "author": "Got It, Inc.",
  "homepage": "https://github.com/gotitinc/aha-icons#readme",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gotitinc/aha-icons.git"
  },
  "description": "Aha Design System - Icons",
  "directories": {
    "lib": "lib"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}
