{
  "parser": "@babel/eslint-parser",
  "parserOptions": {
    "requireConfigFile": false
  },
  "env": {
    "node": true
  },
  "extends": "airbnb",
  "rules": {
    "import/prefer-default-export": 0,
    "import/no-dynamic-require": 0,
    "import/no-unresolved": 0,
    "global-require": 0
  }
}
