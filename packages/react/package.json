{
  "name": "@ahaui/react",
  "version": "2.1.0",
  "description": "Aha Design System - React",
  "homepage": "https://github.com/gotitinc/ahaui/blob/main/packages/react#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gotitinc/ahaui.git"
  },
  "license": "Apache-2.0",
  "author": "Got It, Inc.",
  "main": "dist/index.js",
  "module": "dist",
  "directories": {
    "lib": "lib"
  },
  "files": [
    "es/*.{js,map}",
    "lib/*.{js,map}",
    "types/*.{d.ts}",
    "CHANGELOG.md"
  ],
  "scripts": {
    "build": "pnpm run clean && rollup -c && tsconfig-replace-paths --project tsconfig.dist.json",
    "build-storybook": "build-storybook",
    "clean": "rm -rf dist",
    "dev": "rollup -c -w",
    "format": "eslint --ext js src --fix",
    "postinstall": "pnpm run build",
    "lint": "eslint --ext js src",
    "prepublishOnly": "pnpm run build",
    "storybook": "start-storybook -p 6006",
    "test": "node ../../scripts/app/test.js",
    "test:cov": "node ../../scripts/app/test.js --watchAll=false --coverage --silent"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": "eslint --cache --fix"
  },
  "prettier": {
    "singleQuote": true,
    "trailingComma": "all"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!src/index.ts",
      "!src/**/*.stories.{jsx,tsx}",
      "!src/**/*.d.ts",
      "!src/constants/*",
      "!src/types/*",
      "!src/tests/*",
      "!src/unused/**/*",
      "!src/hooks/mocks/*"
    ],
    "moduleFileExtensions": [
      "js",
      "ts",
      "tsx",
      "json",
      "jsx",
      "node"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web",
      "^.+\\.module\\.(css|sass|scss)$": "identity-obj-proxy",
      "^@[/](.+)": "<rootDir>/src/$1"
    },
    "modulePaths": [
      "<rootDir>/src/"
    ],
    "reporters": [
      "default",
      [
        "../../node_modules/jest-html-reporter",
        {
          "pageTitle": "Aha React Unit Tests",
          "includeConsoleLog": true,
          "includeFailureMsg": true,
          "outputPath": "jest-report.html"
        }
      ]
    ],
    "roots": [
      "<rootDir>/src"
    ],
    "setupFiles": [
      "react-app-polyfill/jsdom"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/src/setupTests.js"
    ],
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "testEnvironment": "jsdom",
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.{js,jsx,ts,tsx}",
      "<rootDir>/src/**/*.{spec,test}.{js,jsx,ts,tsx}"
    ],
    "testPathIgnorePatterns": [
      "<rootDir>/src/unused/"
    ],
    "transform": {
      "^.+\\.(js|jsx|mjs|cjs|ts|tsx)$": "../../config/jest/babelTransform.js",
      "^.+\\.css$": "../../config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|ts|tsx|css|json)$)": "../../config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|ts|tsx)$",
      "^.+\\.module\\.(css|sass|scss)$"
    ],
    "watchPlugins": [
      "jest-watch-typeahead/filename",
      "jest-watch-typeahead/testname"
    ]
  },
  "dependencies": {
    "@typescript-eslint/eslint-plugin": "5.28.0",
    "@typescript-eslint/parser": "5.28.0",
     "eslint": "7.32.0",
    "eslint-config-airbnb": "19.0.4",
    "eslint-plugin-cypress": "2.11.3",
    "eslint-plugin-react-hooks": "4.5.0",
    "eslint-webpack-plugin": "3.1.1",
    "eslint-plugin-import": "2.25.2",
    "eslint-plugin-jsx-a11y": "6.5.1",
    "eslint-plugin-react": "7.28.0",
    "@popperjs/core": "^2.11.7",
    "@restart/hooks": "0.3.26",
    "@rollup/plugin-alias": "3.1.9",
    "@rollup/plugin-babel": "5.3.1",
    "@rollup/plugin-typescript": "8.2.5",
    "@types/lodash": "4.14.182",
    "@types/node": "17.0.41",
    "@types/react": "18.2.6",
    "@types/react-dom": "18.2.4",
    "@types/react-transition-group": "4.4.6",
    "@types/warning": "3.0.0",
    "classnames": "^2.3.2",
    "dom-helpers": "^5.2.1",
    "get-user-locale": "1.5.1",
    "lodash": "4.17.21",
    "make-event-props": "1.3.0",
    "merge-class-names": "1.4.2",
    "prop-types": "15.7.2",
    "prop-types-extra": "1.1.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-fit": "1.4.0",
    "react-textarea-autosize": "8.3.2",
    "react-toastify": "9.0.8",
    "react-transition-group": "4.4.2",
    "rollup": "2.75.3",
    "rollup-plugin-includepaths": "0.2.4",
    "rollup-plugin-node-externals": "4.0.0",
    "tsconfig-replace-paths": "0.0.14",
    "tslib": "2.4.0",
    "typescript": "4.6.3",
    "uncontrollable": "7.1.1",
    "update-input-width": "1.2.2",
    "warning": "4.0.3"
  },
  "devDependencies": {
    "@ahaui/css": "2.1.0",
    "@babel/core": "7.21.0",
    "@babel/preset-env": "^7.22.10",
    "@storybook/addon-actions": "6.5.16",
    "@storybook/addon-essentials": "6.5.16",
    "@storybook/addon-interactions": "6.5.16",
    "@storybook/addon-links": "6.5.16",
    "@storybook/builder-webpack5": "6.5.16",
    "@storybook/manager-webpack5": "6.5.16",
    "@storybook/react": "6.5.16",
    "@storybook/testing-library": "0.0.13",
    "@testing-library/cypress": "9.0.0",
    "@testing-library/dom": "9.3.0",
    "@testing-library/jest-dom": "5.16.5",
    "@testing-library/react-hooks": "8.0.1",
    "babel-loader": "8.3.0",
    "tsconfig-paths-webpack-plugin": "4.0.0"
  },
  "peerDependencies": {
    "react": "18.x",
    "react-dom": "18.x"
  },
  "publishConfig": {
    "access": "public"
  }
}
