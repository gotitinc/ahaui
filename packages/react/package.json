    "@wojtekmaj/predict-input-value": "1.0.0",
    "@wojtekmaj/react-daterange-picker": "3.3.2",
    "classnames": "2.3.1",
    "dom-helpers": "5.1.0",
    "get-user-locale": "1.5.1",
    "lodash.once": "4.1.1",
    "make-event-props": "1.3.0",
    "merge-class-names": "1.4.2",
    "popper.js": "1.15.0",
    "prop-types": "15.7.2",
    "prop-types-extra": "1.1.0",
    "rc-slider": "8.7.1",
    "rc-tooltip": "3.7.3",
    "react-calendar": "3.7.0",
    "react-date-picker": "8.4.0",
    "react-fit": "1.4.0",
    "react-slick": "0.27.13",
