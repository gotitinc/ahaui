    "*.js": "eslint --cache --fix"
  },
  "prettier": {
    "singleQuote": true,
    "trailingComma": "all"
  },
  "author": "Got It, Inc.",
  "homepage": "https://github.com/gotitinc/ahaui/blob/main/packages/react#readme",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gotitinc/ahaui.git"
  },
  "description": "Aha Design System - React",
  "directories": {
    "lib": "lib"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
