{
  "name": "@ahaui/react",
  "version": "2.1.6",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist/*.*",
    "CHANGELOG.md"
  ],
  "scripts": {
    "types": "tsc --project tsconfig.types.json",
    "build:c": "rollup -c",
    "build:vite": "vite build",
    "build": "pnpm run build:vite && pnpm run types",
    "dev": "pnpm run build:vite --watch && pnpm run types",
    "prepublishOnly": "yarn build",
    "dev:c": "rollup -c -w",
    "format": "eslint --ext js src --fix",
    "lint": "eslint --ext js src"
  },
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@restart/hooks": "0.3.25",
    "@types/react-tagsinput": "3.19.9",
    "@wojtekmaj/predict-input-value": "1.0.0",
    "@wojtekmaj/react-daterange-picker": "2.5.0",
    "classnames": "2.2.6",
    "dom-helpers": "5.1.0",
    "popper.js": "1.15.0",
    "@popperjs/core": "2.11.6",
    "prop-types": "15.7.2",
    "prop-types-extra": "1.1.0",
    "rc-slider": "8.7.1",
    "react-calendar": "3.7.0",
    "react-date-picker": "8.4.0",
    "react-slick": "0.27.13",
    "react-split-pane": "0.1.92",
    "react-tagsinput": "3.19.0",
    "react-textarea-autosize": "7.1.2",
    "react-time-picker": "3.9.0",
    "react-toastify": "7.0.4",
    "react-transition-group": "4.0.0",
    "uncontrollable": "7.1.1",
    "warning": "4.0.3",
    "invariant": "2.2.4",
    "dequal": "2.0.3",
    "lodash.once": "4.1.1"
  },
  "devDependencies": {
    "@types/classnames": "2.2.10",
    "@types/prop-types": "15.7.3",
    "@types/rc-slider": "8.6.3"
  },
  "lint-staged": {
    "*.js": "eslint --cache --fix"
  },
  "author": "Got It, Inc.",
  "homepage": "https://github.com/gotitinc/ahaui/blob/main/packages/react#readme",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gotitinc/ahaui.git"
  },
  "description": "Aha Design System - React",
  "directories": {
    "lib": "lib"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}
