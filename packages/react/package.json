{
  "name": "@ahaui/react3",
  "version": "3.0.2",
  "main": "lib/index.js",
  "module": "es/index.js",
  "types": "types/index.d.ts",
  "files": [
    "es/*.{js,map}",
    "lib/*.{js,map}",
    "types/*.{d.ts}",
